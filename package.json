{
  "name": "@codeforcoffee/simple-state",
  "version": "1.0.0-beta.2",
  "description": "Simple State for SPAs",
  "main": "dist/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "build-tests": "./node_modules/.bin/babel src --out-dir dist",
    "gen-docs": "cat CHANGELOG.md >> README.md && node_modules/.bin/jsdoc src/ -d docs/ -t node_modules/docdash/ --readme README.md",
    "test": "./node_modules/mocha/bin/mocha tests/*test.js --reporter spec --compilers js:@babel/register --require @babel/polyfill"
  },
  "keywords": [
    "state",
    "simple",
    "simple-state",
    "store",
    "data",
    "storage"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/code-for-coffee/simple-state.git"
  },
  "author": "code-for-coffee",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "docdash": "^0.4.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  }
}
